<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.osh.jntest.repository.MasterRepository">

    <select id="countUsersByName" resultType="java.lang.Integer">
        SELECT COUNT(*)
        FROM user
        WHERE user_name = #{name}
    </select>

</mapper>